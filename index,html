    $('#instagram-watcher').on('click', 'li',function (e) {
        e.preventDefault();
        var $this = $(this);
        var $el = [];
        var slides = $this.closest('#instagram-watcher').find('li');
        var slideID = $this.attr('data-index') || 0;
        slides.each(function (i, e) {
            var $img = $(e).attr('data-src');
            $el[i] = {
              src: $img,
              thumb: $(e).find('.gallery-item').attr('data-thumb'),
            };
    });

        $this.lightGallery({
            dynamic: true,
            dynamicEl: $el,
            download: false,
            actualSize: false,
            fullScreen: false,
            autoplay: false,
            share: false,
            hideBarsDelay: 3000,
            index: parseInt(slideID, 10),
        });
    });
}
        
        
                                    
